  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "brfs": "^1.4.3",
    "browserify": "^13.1.1",
    "budo": "^9.2.1",
    "chai": "^3.5.0",
    "eslint": "^5.15.1",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
